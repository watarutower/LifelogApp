<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.lifelogapp.ui.home.HomeFragment"

        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_activitylog"
        android:name="com.example.lifelogapp.ui.activitylog.ActivityLogFragment"

        tools:layout="@layout/fragment_activitylog" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.lifelogapp.ui.history.HistoryFragment"

        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.example.lifelogapp.ui.mypage.MyPageFragment"
        tools:layout="@layout/fragment_mypage" />
</navigation>