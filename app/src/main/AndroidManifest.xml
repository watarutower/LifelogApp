<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapplication.lifelogapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_lifelog2_018__1_"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_lifelog2_018__1_"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.myapplication.lifelogapp.MainActivity"
  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name="com.myapplication.lifelogapp.receiver.AlarmReceiver"
            android:enabled="true"
            android:exported="false">
        </receiver>
    </application>

</manifest>